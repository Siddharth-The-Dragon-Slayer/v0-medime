# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Arduino IoT Device Configuration
# Set this to your NodeMCU ESP8266 IP address
# You can find this in the Serial Monitor when the Arduino connects to WiFi
ARDUINO_IP_ADDRESS=192.168.1.100

# Twilio Configuration (Optional - for SMS reminders)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number
